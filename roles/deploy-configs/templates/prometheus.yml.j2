global:
  scrape_interval: 15s

scrape_configs:

  - job_name: 'prometheus'
    scrape_interval: 5s
    static_configs:
      - targets: ['localhost:9090']

  - job_name: 'node_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.1.80:9100']
      - targets: ['192.168.1.72:9100']
      - targets: ['192.168.1.117:9100']
      - targets: ['192.168.1.118:9100']